    {
      "name": "Jisse Reitsma (Yireo)",
      "email": "jisse@yireo.com"
    }
  ],
  "require": {
    "magento/framework": "^100.1|^101.0|^102.0",
    "filp/whoops": "^2.1",
    "symfony/var-dumper": "*",
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "composer/composer": "*@dev"
